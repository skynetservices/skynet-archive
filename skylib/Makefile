include $(GOROOT)/src/Make.inc

TARG=github.com/bketelsen/skynet/skylib
GOFILES=\
	config.go\
	constants.go\
	doc.go\
	log.go\
	routing.go\
	service.go\
	types.go\

# gb: this is the local install
GBROOT=..

# gb: compile/link against local install
GC+= -I $(GBROOT)/_obj
LD+= -L $(GBROOT)/_obj

# gb: copy to local install
$(GBROOT)/_obj/$(TARG).a: _obj/$(TARG).a
